<style>
  @import url('https://fonts.googleapis.com/css?family=Noto+Sans+KR:400,700');
  html {
  -webkit-font-smoothing: antialiased;
  }
  body, p, a, span, h1, h2, h3, h4, h5, h6, a, div {
    font-family: 'Noto Sans KR', sans-serif !important;
  }
</style>
<% if @is_mobile %>
  <div data-collapse="medium" data-animation="default" data-duration="500" data-easing="ease-out-expo" data-easing2="ease-in-out" data-no-scroll="1" id="nav" class="nav-cz w-nav">
    <div class="wrapper w-container">
       <div class="nav-body">
          <a href="/" class="nav-brand w-nav-brand">
             <% if @is_mobile %>
                <p style="font-weight: 550;padding-top:28px; padding-bottom: auto;">Chack N Zzick</p>
  
             <% else %> 
                <p style="font-weight: 550;padding-top:43px; padding-bottom: auto;">Chack N Zzick</p>
             <% end %>
          </a>
          
          
          <nav role="navigation" class="nav-menu w-nav-menu">
             <a href="/paintings" class="nav-item is-last-h w-nav-link">Paintings</a>
             <a href="/web_magazines" class="nav-item is-last-h w-nav-link">Magazine</a>   
             <a href="/about" class="nav-item is-last-h w-nav-link">About</a>
             <a href="/notices" class="nav-item is-last-h w-nav-link">Notice</a>
             <a href="/faqs" class="nav-item is-last-h w-nav-link">FAQ</a>
             
            
               
             <% if user_signed_in? %>
                <%= link_to 'Mypage', profiles_path, class:"nav-item is-last-h w-nav-link" %>
