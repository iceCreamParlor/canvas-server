               <div class="dropdown nav-item is-last-h w-nav-link">
                  <%= current_user.nickname %>
                  <div class="dropdown-content">
                     <%= link_to 'My page', profile_path(current_user.id) %>                    
                     <%= link_to 'Account', profiles_path, class:"nav-item is-last-h w-nav-link" %>
                     <%= link_to 'Logout', destroy_user_session_path, class:"nav-item is-last-h w-nav-link", method: :delete %>
                     <%= link_to 'Message', messages_path %>
                     <%= link_to 'Orders', order_list_orders_path, class: "white" %>
                     <%= link_to 'Cart', cart_orders_path, class: "white" %>
                  </div>
               </div>
             <% else %>
                <!-- <a href="/users/sign_in" class="nav-item is-last-h w-nav-link">Login</a> -->
                <%= link_to 'Login',
                  '', 
                  data: { toggle: 'modal', target: '#login-modal' },
                  class:"nav-item is-last-h w-nav-link"
                %>
             <% end %>
          </nav>
          <div data-w-id="b2f9f39b-9e6e-8e20-21eb-f0d9828fe86c" class="nav-toggle w-nav-button"><img src="/img/5b13ec2c541a846f4d141dd0_icon-menu.svg" width="28" data-w-id="b2f9f39b-9e6e-8e20-21eb-f0d9828fe86d" style="-webkit-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" alt="" class="nav-icon"/><img src="/img/5b13ef75541a841daa141eec_icon-close.svg" data-w-id="b2f9f39b-9e6e-8e20-21eb-f0d9828fe86e" style="-webkit-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" alt="" class="nav-icon-close"/></div>
       </div>
      <div class="hr hr-thin" style="float: left;"></div>   
    </div>
    
