    </div>
  </div>
<% else %>
  <div data-collapse="medium" data-animation="default" data-duration="500" data-easing="ease-out-expo" data-easing2="ease-in-out" data-no-scroll="1" id="nav"
  class="nav-cz w-nav <% if controller_name=='web_magazines' %>margin-sidebar<% end %>">
    <!-- 좌측 sidebar 때문에 left-margin 줘야 함 --> 
    <div class="wrapper w-container" >
       <div class="nav-body">
          <a href="/" class="nav-brand w-nav-brand">
             <% if @is_mobile %>
                <p style="font-weight: 550;padding-top:28px; padding-bottom: auto;">Chack N Zzick</p>
             <% else %> 
                <p style="font-weight: 550;padding-top:43px; padding-bottom: auto;">Chack N Zzick</p>
             <% end %>
          </a>
          <a href="" class="nav-item is-last-h w-nav-link nav-left">|</a>
          <div class="dropdown nav-item is-last-h w-nav-link nav-left">Paintings
            <div class="dropdown-content">
               <%= link_to 'Package', paintings_path(only_commerce: true) %>
               <%= link_to 'All', paintings_path %>
            </div>
          </div>
          <a href="/web_magazines" class="nav-item is-last-h w-nav-link nav-left">Magazine</a>
          <nav role="navigation" class="nav-menu w-nav-menu">
             <a href="/about" class="nav-item is-last-h w-nav-link">About</a>
             <a href="/notices" class="nav-item is-last-h w-nav-link">Notice</a>
